<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.policestrategies.calm_stop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name_officer"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".officer.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".officer.LoginActivity" />

        <activity
            android:name=".officer.SignupActivity" />

        <activity
            android:name=".officer.HomepageActivity" />

        <activity
            android:name=".officer.AccountActivity" />

        <activity
            android:name=".officer.RatingActivity" />

        <activity
            android:name=".officer.HistoryActivity" />

        <activity
            android:name=".officer.AboutUsActivity" />

        <activity
            android:name=".officer.HelpActivity" />

        <activity
            android:name=".officer.ProfileActivity" />

        <activity
            android:name=".officer.SettingsActivity" />


    </application>

</manifest>